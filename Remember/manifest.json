{
  "manifest_version": 2,
  "name": "Remember",
  "description": "Saves highlighted text into excel",
  "version": "1.1",
  "author": "Kevin Durr",
  "icons":{
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Remember",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab", 
    "contextMenus",
    "http://127.0.0.1:5000/input?q=*"
  ],
  "background": {
    "scripts": ["jquery.js","background.js", "eventPage.js"]
  }
}